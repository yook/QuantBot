{
  "name": "",
  "url": "",
  "freezed": false,
  "stats": {
    "fetched": 0,
    "queue": 0,
    "disallow": 0,
    "html": 0,
    "jscss": 0,
    "image": 0,
    "redirect": 0,
    "error": 0,
    "depth3": 0,
    "depth5": 0,
    "depth6": 0,
    "other": 0
  },
  "crawler": {
    "maxDepth": 1,
    "maxConcurrency": 1,
    "interval": 1000,
    "timeout": 10000,
    "parseScriptTags": true,
    "parseImages": false,
    "stripQuerystring": false,
    "sortQueryParameters": true,
    "respectRobotsTxt": true,
    "scanSubdomains": false,
    "userAgent": "Mozilla/5.0 (compatible; Googlebot/2.1; +http://www.google.com/bot.html)"
  },
  "parser": [
    {
      "name": "Title",
      "prop": "title",
      "selector": "title",
      "find": "text",
      "attrClass": "",
      "getLength": false
    },
    {
      "name": "Title length",
      "prop": "titleLength",
      "selector": "title",
      "find": "text",
      "attrClass": "",
      "getLength": true
    },
    {
      "name": "H1",
      "prop": "h1",
      "selector": "h1",
      "find": "text",
      "attrClass": "",
      "getLength": false
    },
    {
      "name": "H1 quantity",
      "prop": "h1quantity",
      "selector": "h1",
      "find": "quantity",
      "attrClass": "",
      "getLength": false
    },
    {
      "name": "Description",
      "prop": "description",
      "selector": "meta[name=description]",
      "find": "attr",
      "attrClass": "content",
      "getLength": false
    },
    {
      "name": "Description length",
      "prop": "descriptionLength",
      "selector": "meta[name=description]",
      "find": "attr",
      "attrClass": "content",
      "getLength": true
    },
    {
      "name": "Links quantity",
      "prop": "linksLength",
      "selector": "a[href]",
      "find": "quantity",
      "attrClass": "",
      "getLength": false
    }
  ],
  "columns": {
    "html": [
      "url",
      "referrer",
      "depth",
      "code",
      "contentType",
      "protocol",
      "actualDataSize",
      "requestTime",
      "requestLatency",
      "downloadTime",
      "status",
      "date",
      "title",
      "titleLength",
      "h1",
      "h1quantity",
      "description",
