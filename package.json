{
  "name": "quantbot",
  "productName": "QuantBot",
  "description": "QuantBot — инструмент для краулинга и анализа сайтов",
  "author": "Yook",
  "private": true,
  "version": "1.3.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate:icons": "node ./build-scripts/generate-icons.js",
    "build": "vue-tsc && vite build && electron-builder",
    "build:win:x64": "vue-tsc && vite build && electron-builder --win --x64",
    "build:win:ia32": "vue-tsc && vite build && electron-builder --win --ia32",
    "build:win:multiarch": "npm run build:win:x64 && npm run build:win:ia32",
    "build:win:portable": "vue-tsc && vite build && electron-builder --win portable --x64",
    "build:win": "vue-tsc && vite build && electron-builder --win --x64",
    "build:win:cross": "vue-tsc && vite build && electron-builder --win --x64 --config.extraMetadata.main=dist-electron/main.js",
    "build:win:portable:cross": "vue-tsc && vite build && electron-builder --win portable --x64 --config.extraMetadata.main=dist-electron/main.js",
    "build:win:portable:ci": "vite build && electron-builder --win portable --x64 --config.extraMetadata.main=dist-electron/main.js",
    "preview": "vite preview",
    "electron:dev": "vite --mode electron",
    "rebuild:sqlite": "bash ./scripts/rebuild-sqlite.sh",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@electron/asar": "^4.0.1",
    "@vscode/sqlite3": "^5.1.10-vscode",
    "axios": "^1.13.2",
    "better-sqlite3": "^12.5.0",
    "bindings": "^1.5.0",
    "body-parser": "^2.2.1",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "electron-updater": "^6.6.2",
    "element-plus": "^2.12.0",
    "express": "^5.2.1",
    "file-uri-to-path": "^2.0.0",
    "http": "^0.0.1-security",
    "moment": "^2.30.1",
    "pinia": "^3.0.4",
    "phpmorphy": "^2.3.2",
    "simplecrawler": "^1.1.9",
    "validator": "^13.15.23",
    "vue": "^3.5.25",
    "vue-i18n": "^11.2.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@vitejs/plugin-vue": "^6.0.2",
    "autoprefixer": "^10.4.22",
    "electron": "^39.2.6",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "icon-gen": "^5.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vue-tsc": "^3.1.8"
  },
  "main": "dist-electron/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yook/QuantBot"
  }
}
